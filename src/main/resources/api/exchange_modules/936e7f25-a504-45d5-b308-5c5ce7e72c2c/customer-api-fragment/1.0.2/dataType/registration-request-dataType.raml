#%RAML 1.0 DataType

type: object
properties:
  registrationId:
    required: false
    type: string
    example: "customer1001"
  name:
    required: true
    type: string
    example: "john paul"
  address:
    required: true
    type: string
    example: "Hyderabad"
  mobile:
    required: true
    type: string
    example: "+91-9876543210"
  email:
    required: true
    type: string
    example: "john@paul.com"
  